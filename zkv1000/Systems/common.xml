<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <animation>
    <!--object-name>map.background</object-name>
    <object-name>map.aircraft</object-name>
    <object-name>map.legend</object-name>
    <object-name>map.alt</object-name>
    <object-name>map.alt.1min</object-name>
    <object-name>map.alt.selected</object-name-->
    <object-name>knob.RANGE.push</object-name>
    <object-name>knob.RANGE.up</object-name>
    <object-name>knob.RANGE.right</object-name>
    <object-name>knob.RANGE.down</object-name>
    <object-name>knob.RANGE.left</object-name>
    <object-name>softkey.D</object-name>
    <object-name>softkey.MENU</object-name>
    <object-name>softkey.FPL</object-name>
    <object-name>softkey.PROC</object-name>
    <object-name>softkey.CLR</object-name>
    <type>select</type>
    <condition>
      <equals>
        <property>/instrumentation/zkv1000/device[1]/status</property>
        <value>2</value>
      </equals>
    </condition>
  </animation>

  <!-- ATTITUDE INDICATOR -->
  <animation>
    <object-name>ai.pitch.indicator</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/orientation/pitch-deg</property>
      <factor>0.005</factor>
      <center>
        <x>0.5</x>
        <y>0.5</y>
      </center>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>texrotate</subtype>
      <property>/orientation/roll-deg</property>
      <min>-180</min>
      <max> 180</max>
      <center>
        <x>0.5</x>
        <y>0.5</y>
      </center>
      <axis>
        <x>0</x>
        <y>0</y>
        <z>-1</z>
      </axis>
    </transform>
  </animation>

  <animation>
    <object-name>ai.background</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/orientation/pitch-deg</property>
      <factor>0.003</factor>
      <center>
        <x>0.5</x>
        <y>0.5</y>
      </center>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>texrotate</subtype>
      <property>/orientation/roll-deg</property>
      <min>-180</min>
      <max> 180</max>
      <center>
        <x>0.5</x>
        <y>0.5</y>
      </center>
      <axis>
        <x>0</x>
        <y>0</y>
        <z>-1</z>
      </axis>
    </transform>
  </animation>

  <animation>
    <object-name>ai.roll.ref</object-name>
    <type>rotate</type>
    <property>/orientation/roll-deg</property>
    <center>
      <x-m> 0.061</x-m>
      <y-m>-0.011</y-m>
      <z-m> 0.027</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>ai.fd.bars</object-name>
    <type>select</type>
    <condition>
      <property>/instrumentation/zkv1000/afcs/fd-bars-visible</property>
    </condition>
  </animation>

  <animation>
    <object-name>ai.fd.bars</object-name>
    <type>rotate</type>
    <property>/autopilot/internal/target-roll-deg</property>
    <center>
      <x-m> 0.061</x-m>
      <y-m>-0.011</y-m>
      <z-m> 0.027</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>ai.fd.bars</object-name>
    <type>rotate</type>
    <property>/orientation/roll-deg</property>
    <center>
      <x-m> 0.061</x-m>
      <y-m>-0.011</y-m>
      <z-m> 0.027</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>ai.fd.bars</object-name>
    <type>translate</type>
    <property>/orientation/pitch-deg</property>
    <factor>0.00135</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <object-name>ai.fd.bars</object-name>
    <type>translate</type>
    <property>/autopilot/internal/target-pitch-deg</property>
    <factor>0.00135</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!-- AIRSPEED INDICATOR -->
  <animation>
    <object-name>asi.tape</object-name>
    <type>textranslate</type>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <factor>0.0025</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>asi.tape</object-name>
    <type>material</type>
    <property-base>/instrumentation/zkv1000</property-base>
    <texture-prop>asi-tape-texture</texture-prop>
  </animation>

  <animation>
    <object-name>asi.cursor.100</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
      <step>100</step>
      <factor>0.001</factor>
      <axis>
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/ias[0]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>asi.cursor.010</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
      <step>10</step>
      <factor>0.01</factor>
      <axis>
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/ias[1]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>asi.cursor.001</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
      <factor>0.1</factor>
      <axis>
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/ias[2]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>infos.FLC</object-name>
    <object-name>asi.bug</object-name>
    <type>select</type>
    <condition>
      <equals>
        <property>/autopilot/locks/pitch</property>
        <value>FLC</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <object-name>asi.bug</object-name>
    <type>translate</type>
    <property>/instrumentation/zkv1000/afcs/selected-ias-kt-diff</property>
    <interpolation>
      <entry><ind>-30</ind><dep>-0.0331</dep></entry>
      <entry><ind> 0   </ind><dep> 0   </dep></entry>
      <entry><ind> 30</ind><dep> 0.0331</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!-- ALTITUDE INDICATOR -->
  <animation>
    <object-name>alt.tape</object-name>
    <type>textranslate</type>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <factor>0.0000437</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <object-name>alt.cursor.10000</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/altimeter/indicated-altitude-ft</property>
      <step>10000</step>
      <factor>-0.00001</factor>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/alt[0]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>alt.cursor.01000</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/altimeter/indicated-altitude-ft</property>
      <step>1000</step>
      <factor>-0.0001</factor>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/alt[1]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>alt.cursor.00100</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/altimeter/indicated-altitude-ft</property>
      <step>100</step>
      <factor>-0.001</factor>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cursors/alt[2]</property>
      <factor>0.125</factor>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>    
    </transform>
  </animation>

  <animation>
    <object-name>alt.cursor.00011</object-name>
    <type>textranslate</type>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <factor>-0.01</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>infos.ALT</object-name>
    <object-name>alt.bug</object-name>
    <type>select</type>
    <condition>
      <property>/instrumentation/zkv1000/afcs/alt-bug-visible</property>
    </condition>
  </animation>

  <animation>
    <object-name>alt.bug</object-name>
    <type>translate</type>
    <property>/instrumentation/zkv1000/afcs/selected-alt-ft-diff</property>
    <interpolation>
      <entry><ind>-300</ind><dep>-0.0358</dep></entry>
      <entry><ind> 0   </ind><dep> 0   </dep></entry>
      <entry><ind> 300</ind><dep> 0.0358</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!-- VERTICAL SPEED INDICATOR -->
  <animation>
    <object-name>alt.vs.ptr</object-name>
    <type>translate</type>
    <property>/velocities/vertical-speed-fps</property>
    <interpolation>
      <entry><ind>-33.3333333</ind><dep>-0.03</dep></entry>
      <entry><ind> 0   </ind><dep> 0   </dep></entry>
      <entry><ind> 33.3333333</ind><dep> 0.03</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <animation>
    <object-name>alt.vs.bug</object-name>
    <type>select</type>
    <condition>
      <equals>
        <property>/instrumentation/zkv1000/afcs/pit-active-mode</property>
        <value>3</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <object-name>alt.vs.bug</object-name>
    <type>translate</type>
    <property>/autopilot/settings/vertical-speed-fpm</property>
    <interpolation>
      <entry><ind>-2000</ind><dep>-0.03</dep></entry>
      <entry><ind> 0   </ind><dep> 0   </dep></entry>
      <entry><ind> 2000</ind><dep> 0.03</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!-- COMPASS ROSE -->
  <animation>
    <object-name>hsi.compass</object-name>
    <object-name>hsi.arc</object-name>
    <type>texrotate</type>
    <property>/orientation/heading-magnetic-deg</property>
    <center>
      <x>0.5</x>
      <y>0.5</y>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <object-name>hsi.bezel.turn.right</object-name>
    <type>texrotate</type>
    <property>/instrumentation/turn-indicator/indicated-turn-rate</property>
    <factor>30</factor>
    <min>0</min>
    <max>1.2</max>
    <center>
      <x>0.5</x>
      <y>0.5</y>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <animation>
    <object-name>hsi.bezel.turn.left</object-name>
    <type>texrotate</type>
    <property>/instrumentation/turn-indicator/indicated-turn-rate</property>
    <factor>30</factor>
    <min>-1.2</min>
    <max>0</max>
    <center>
      <x>0.5</x>
      <y>0.5</y>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!-- HORIZONTAL SITUATION INDICATOR -->
  <animation>
    <object-name>hsi.crs.pointer</object-name>
    <object-name>hsi.crs.deflect.ref</object-name>
    <object-name>hsi.crs.plane</object-name>
    <object-name>hsi.BRG1</object-name>
    <type>select</type>
    <condition>
      <property>/instrumentation/zkv1000/cdi/visible</property>
    </condition>
  </animation>

  <animation>
    <object-name>hsi.crs.deflection</object-name>
    <type>select</type>
    <condition>
      <and>
        <property>/instrumentation/zkv1000/cdi/visible</property>
        <property>/instrumentation/zkv1000/cdi/in-range</property>
      </and>
    </condition>
  </animation>

  <animation>
    <object-name>hsi.crs.deflection</object-name>
    <object-name>hsi.crs.pointer</object-name>
    <type>texmultiple</type>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cdi/pointer-type</property>
      <factor>0.166666667</factor>
      <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/cdi/from-flag</property>
      <factor>0.166666667</factor>
      <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </transform>
  </animation>

  <animation>
    <object-name>hsi.crs.pointer</object-name>
    <object-name>hsi.crs.deflect.ref</object-name>
    <object-name>hsi.crs.deflection</object-name>
    <object-name>hsi.bug.HDG</object-name>
    <type>rotate</type>
    <property>/orientation/heading-magnetic-deg</property>
    <center>
      <x-m> 0.004</x-m>
      <y-m>-0.011</y-m>
      <z-m>-0.038</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>hsi.crs.pointer</object-name>
    <object-name>hsi.crs.deflect.ref</object-name>
    <object-name>hsi.crs.deflection</object-name>
    <type>rotate</type>
    <property>/instrumentation/zkv1000/cdi/course</property>
    <center>
      <x-m> 0.004</x-m>
      <y-m>-0.011</y-m>
      <z-m>-0.038</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>hsi.crs.deflection</object-name>
    <type>translate</type>
    <property>/instrumentation/zkv1000/cdi/course-deflection</property>
    <factor>0.006</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>hsi.bug.HDG</object-name>
    <type>rotate</type>
    <property>/instrumentation/zkv1000/afcs/heading-bug-deg</property>
    <center>
      <x-m> 0.062</x-m>
      <y-m>-0.011</y-m>
      <z-m>-0.038</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <object-name>knob.HDG</object-name>
    <type>pick</type>
    <visible>false</visible>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.HDGknob(0);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
    <action>
      <button>0</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.HDGknob(1);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
    <action>
      <button>2</button>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.HDGknob(-1);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
  </animation>

  <animation>
    <object-name>knob.NAV.small</object-name>
    <object-name>knob.NAV.large</object-name>
    <type>pick</type>
    <visible>false</visible>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.NAVknob(0);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
  </animation>

  <animation>
    <object-name>knob.NAV.small</object-name>
    <type>pick</type>
    <visible>false</visible>
    <action>
      <button>0</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.NAVknob(0.05);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
    <action>
      <button>2</button>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.NAVknob(-0.05);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
  </animation>

  <animation>
    <object-name>knob.NAV.large</object-name>
    <type>pick</type>
    <visible>false</visible>
    <action>
      <button>0</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.NAVknob(1);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
    <action>
      <button>2</button>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.NAVknob(-1);</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property type="bool">/devices/status/mice/mouse[0]/freezed</property>
        <value type="bool">true</value>
      </binding>
    </action>
  </animation>

  <animation>
    <object-name>softkey.NAV.swap</object-name>
    <type>pick</type>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>zkv1000.swapNAV();</script>
      </binding>
