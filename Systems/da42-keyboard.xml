<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <keyboard>
    
    <key n="100">
      <name>d</name>
      <desc>Crew door</desc>
      <binding>
        <command>property-toggle</command>
        <property>controls/doors/door[0]/locked</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>
    
    <key n="101">
      <name>e</name>
      <desc>Left bagage door</desc>
      <binding>
        <command>property-toogle</command>
        <property>controls/doors/door[2]/locks/lock[0]/locked</property>
      </binding>
      <binding>
        <command>property-toogle</command>
        <property>controls/doors/door[2]/locks/lock[1]/locked</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>doors.leftbagage.toggle();</script>
      </binding>
    </key>
    
    <key n="68">
      <name>D</name>
      <desc>Passenger door</desc>
      <binding>
        <command>property-toogle</command>
        <property>controls/doors/door[1]/locked</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>doors.passenger.toggle();</script>
      </binding>
    </key>
    
    <key n="69">
      <name>E</name>
      <desc>Right bagage door</desc>
      <binding>
        <command>property-toogle</command>
        <property>controls/doors/door[3]/locks/lock[0]/locked</property>
      </binding>
      <binding>
        <command>property-toogle</command>
        <property>controls/doors/door[3]/locks/lock[1]/locked</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>doors.rightbagage.toggle();</script>
      </binding>
    </key>
    
    <key n="108">
      <name>l</name>
      <desc>Landing light</desc>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/landing-lights</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>/sim/rendering/als-secondary-lights/use-landing-light</property>
      </binding>
    </key>
    
    <key n="76">
      <name>L</name>
      <desc>Taxi light</desc>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/taxi-lights</property>
      </binding>
    </key>
    
  </keyboard>
  
</PropertyList>
