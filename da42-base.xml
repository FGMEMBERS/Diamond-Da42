<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <systems include="Nasal/electrical.xml">
		<autopilot>
            <path>Aircraft/Diamond-Da42/zkv1000/Systems/autopilot.xml</path>
        </autopilot>
	</systems>

	<instrumentation>
      <path>Aircraft/Diamond-Da42/Nasal/instrumentation.xml</path>
    </instrumentation>

    <sound>
      <path archive="y">Aircraft/Diamond-Da42/Sounds/da42-sound.xml</path>
    </sound>

    <model>
      <path archive="y">Aircraft/Diamond-Da42/Models/da42.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
	  
	  <!--taken from Seneca II and adapted-->
	  <icing>
        <iceable>
          <name>Wings</name>
          <salvage-control>/controls/deice/mode</salvage-control>
          <sensitivity type="double">0.8</sensitivity>
          <output-property>/sim/icing/wings</output-property>
        </iceable>
        <iceable>
          <name>Windshield</name>
          <salvage-control>/controls/deice/mode_windshield</salvage-control>
          <sensitivity type="double">0.5</sensitivity>
          <output-property>/sim/icing/windshield</output-property>
        </iceable>
        <iceable>
          <name>Fuselage</name>
          <!-- no salvage control, no deicing of the fuselage -->
          <sensitivity type="double">1.0</sensitivity>
          <output-property>/sim/icing/other</output-property>
        </iceable>
		<iceable>
          <name>Engines</name>
          <!-- no salvage control, no deicing of the fuselage -->
          <sensitivity type="double">1.0</sensitivity>
          <output-property>/sim/icing/engines</output-property>
        </iceable>
		<iceable>
          <name>Propellers</name>
          <salvage-control>/controls/deice/mode</salvage-control>
          <sensitivity type="double">0.5</sensitivity>
          <output-property>/sim/icing/propellers</output-property>
        </iceable>
        <iceable>
          <name>Pitot tube</name>
          <sensitivity type="double">1</sensitivity>
          <salvage-control>/instrumentation/pitot-heat/active</salvage-control>
          <output-property>/sim/icing/pitot</output-property>
        </iceable>
      </icing>
    </model>

	
    <flaps>
      <setting> 0.000 </setting>
      <setting> 0.375 </setting>
      <setting> 1.000 </setting> <!-- 20 deg -->
    </flaps>

    <multiplay>
      <chat_display>1</chat_display>
      <generic> 
        <int type="int" n="0">  0 </int>                                      <!-- 0 immat -->
        <int type="int" n="1">  0 </int>                                      <!-- 1 immat -->
        <int type="int" n="2">  0 </int>                                      <!-- 2 immat -->
        <int type="int" n="3">  0 </int>                                      <!-- 3 immat -->
        <int type="int" n="4">  0 </int>                                      <!-- 4 immat -->
        <int type="int" n="5">  0 </int>                                      <!-- 5 immat -->
        <int type="bool" n="6"> 1 </int>                                      <!-- 6 smoke/bomb/torpedo -->
        <int type="int" n="7">  0 </int>                                      <!-- 7 color -->
        <int n="8" alias="/systems/electrical/outputs/nav-lights">  0 </int>  <!-- 8 nav-lights -->
        <int n="9" alias="/sim/model/lights/beacon/state">  false </int>      <!-- 9 strobe-lights -->
        <int n="10" alias="/controls/electric/key-pos">  0 </int>             <!-- 10 key position -->
        <float n="2" alias="/position/gear-agl-m">  0 </float>                <!-- 11 altitude for MP shadow -->
        <float n="0" alias="/instrumentation/doors/crew/position-norm"/>
        <float n="1" alias="/instrumentation/doors/passenger/position-norm"/>  
        <float n="3" alias="/instrumentation/doors/leftbagage/position-norm"/>  
        <float n="4" alias="/instrumentation/doors/rightbagage/position-norm"/>  
      </generic>
    </multiplay>

    <help>
      <title>Diamond Da42</title>
      <key>
        <name>d</name>
        <desc>Open/Close crew canopy</desc>
      </key>
      <key>
        <name>D</name>
        <desc>Open/Close passenger door</desc>
      </key>
    </help>

    <menubar include="Dialogs/da42-menu.xml"/>

	<!--weights-->
    <weight n="0">
      <name type="string">Pilot</name>
