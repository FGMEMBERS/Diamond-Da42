<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <default>
    <menu n="10">
      <label>Diamond DA 42 Twin Star</label>
      <enabled type="bool">true</enabled>
	  <item>
        <label>Auto Start</label>
        <binding>
          <command>nasal</command>
          <script>da42.autostart();</script>
        </binding>
      </item>
      <item>
        <label>Select Livery</label>
        <binding>
          <command>nasal</command>
          <script>aircraft.livery.dialog.toggle();</script>
        </binding>
      </item>
      <item>
        <label>Immatriculation</label>
        <binding>
          <command>nasal</command>
          <script>da42.immat_dialog.open();</script>
        </binding>
      </item>
	  <item>
        <label>Show/hide pilot</label>
        <binding>
          <command>nasal</command>
          <script>da42.togglePilot();</script>
        </binding>
      </item>
	  <item>
		<label>Pushback</label>
		<binding>
			<command>dialog-show</command>
			<dialog-name>pushback</dialog-name>
		</binding>
		</item>
	<item>
		<label>Test Icing</label>
		<binding>
		<command>nasal</command>
		<script>da42.testIcing();</script>
		</binding>
	</item>
	<item>
		<label>Toogle Rembrandt</label>
		<binding>
		<command>nasal</command>
		<script>da42.toogleRembrandt();</script>
		</binding>
	</item>
	<item>
		<label>Toogle Glass Reflection</label>
		<binding>
		<command>nasal</command>
		<script>da42.toogleShaders();</script>
		</binding>
	</item>
    </menu>
  </default>


</PropertyList>

