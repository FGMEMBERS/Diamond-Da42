<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>handledoor.ac</path>
  
  <animation>
    <object-name>support</object-name>
    <object-name>lock</object-name>
  </animation>
  
  <animation>
    <type>pick</type>
    <object-name>support</object-name>
    <object-name>lock</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/doors/door[1]/locked</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>lock</object-name>
    <property>controls/doors/door[1]/locked</property>
    <factor> -90 </factor>
    <center>
      <x-m>  0.016630 </x-m>
      <y-m> -0.002696 </y-m>
      <z-m>  0.000279 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
</PropertyList>
