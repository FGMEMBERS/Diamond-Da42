<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

	<path>greenlight.ac</path>

	<animation>
		<type>material</type>
		<object-name>light</object-name>
		<emission>
			<red>0</red>
			<green>1</green>
			<blue>0</blue>
		</emission>
		<condition>
			<and>
				<greater-than>
					<property>/surface-positions/flaps-pos-norm</property>
					<value type="double">-0.25</value>
				</greater-than>
				<equals>
					<property>/controls/electric/alimentation/flap</property>
					<value>1</value>
				</equals>
			</and>
		</condition>
	</animation>
	
</PropertyList>


