		</interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
	
	<animation>
		<type>pick</type>
		<object-name>rightlever</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-adjust</command>
				<property>/controls/fuel/tank[1]/position</property>
				<step>1</step>
				<min>0</min>
				<max>2</max>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>/sim/sound/switch</property>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>rightlever</object-name>
		<visible>true</visible>
		<action>
			<button>1</button>
			<binding>
				<condition>
					<or>
						<equals>
