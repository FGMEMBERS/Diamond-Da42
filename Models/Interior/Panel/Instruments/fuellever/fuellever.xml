<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

	<path>fuellever.ac</path>
	
	<animation>
        <type>rotate</type>
        <object-name>leftlock</object-name>
        <property>/controls/fuel/tank[0]/lock</property>
        <factor>90</factor>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>-1.0</z>
        </axis>
		<center>
			<x-m>0.0156</x-m>
			<y-m>-0.0144</y-m>
			<z-m>-0.0049</z-m>
		</center>
    </animation>
	
	<animation>
		<type>pick</type>
		<object-name>leftlock</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<greater-than>
						<property>/controls/fuel/tank[0]/position</property>
						<value type="double">0.0</value>
					</greater-than>
				</condition>
				<command>property-toggle</command>
				<property>/controls/fuel/tank[0]/lock</property>
			</binding>
		</action>
	</animation>
	
	<animation>
        <type>translate</type>
        <object-name>leftlever</object-name>
        <property>/controls/fuel/tank[0]/position</property>
		<interpolation>
			<entry>
				<ind> 0 </ind>
				<dep>   0 </dep>
			</entry>
			<entry>
				<ind> 1 </ind>
				<dep>0.013 </dep>
			</entry>
			<entry>
				<ind> 2 </ind>
				<dep> 0.06 </dep>
			</entry>
		</interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
	
	<animation>
		<type>pick</type>
		<object-name>leftlever</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-adjust</command>
				<property>/controls/fuel/tank[0]/position</property>
				<step>1</step>
				<min>0</min>
				<max>2</max>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>/sim/sound/switch</property>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>leftlever</object-name>
		<visible>true</visible>
		<action>
			<button>1</button>
			<binding>
				<condition>
					<or>
						<equals>
							<property>/controls/fuel/tank[0]/lock</property>
							<value>0</value>
						</equals>
						<greater-than>
							<property>/controls/fuel/tank[0]/position</property>
							<value type="double">1.0</value>
						</greater-than>
					</or>
				</condition>
				<command>property-adjust</command>
				<property>/controls/fuel/tank[0]/position</property>
				<step>-1</step>
				<min>0</min>
				<max>2</max>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>/sim/sound/switch</property>
			</binding>
		</action>
	</animation>
	
	<animation>
        <type>rotate</type>
        <object-name>rightlock</object-name>
        <property>/controls/fuel/tank[1]/lock</property>
        <factor>90</factor>
        <axis>
            <x>0.0</x>
