	<!--futur utilisation-->
	<animation>
		<object-name>knob.Vol.small</object-name>
		<object-name>knob.Vol.large</object-name>
		<type>pick</type>
		<repeatable>false</repeatable>
		<visible>false</visible>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>knob.RED</object-name>-
		<emission>
			<red>1</red>
			<green>0</green>
			<blue>0</blue>
		</emission>
		<condition>
			<property>/instrumentation/audio/serviceable</property>
		</condition>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>body.mixer</object-name>
		<emission>
			<red>0.2</red>
			<green>0.2</green>
			<blue>0.2</blue>
			<factor-prop>/controls/switches/flood-lights-norm</factor-prop>
		</emission>
		<condition>
			<greater-than>
				<property>/controls/lighting/flood-lights</property>
				<value>0</value>
			</greater-than>	
		</condition>
	</animation>
	
	<animation> 
    <type>select</type> 
    <object-name>body.enlighted</object-name> 
    <condition>
      <and>
        <greater-than>
          <property>/instrumentation/zkv1000/body-emission</property>
			<value>0.1</value>
        </greater-than>
       <property>/instrumentation/audio/serviceable</property>
      </and>
    </condition>
  </animation>
